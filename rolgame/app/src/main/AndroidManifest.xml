<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rolgame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rolgameicon"
        android:label="RolGame"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Results"></activity>
        <activity android:name=".Equipment" />
        <activity android:name=".Combat" />
        <activity android:name=".BuyObject" />
        <activity android:name=".Shop" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".Perfil" />
        <activity android:name=".selectClase" />
        <activity android:name=".Registro" />
        <activity android:name=".MainMenu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>